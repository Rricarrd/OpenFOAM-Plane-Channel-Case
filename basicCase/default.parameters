// Simulation parameters
case temporal;
solver pisoFoam;
numberOfProcessors 16;
tStart 0;
tEnd 50;
dt 1e-2;
writeTime 1;
Re_lam 5000;

// Turbulence model
model dynamicSmagorinsky;
ce 1.048;
ck 0.0265463553;

// Orr-Sommerfeld Initial conditions 
Ucl 1;
alpha_2D 1.12;
alpha_3D 1.12;
beta_2D 0;
beta_3D 2.1;
A_2D 3;
A_3D 0.1;
n_2D 0;
n_3D 2;
Np 0;
dynVisc 2e-4;
rho 1;

// Meshing
H 2;
W 2.9919930042;
L 5.60998688141;
nx 32;
ny 32;
nz 33;
resFactor 1.;
graded true;


