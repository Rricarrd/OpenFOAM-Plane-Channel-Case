// Simulation parameters
case temporal;
solver pisoFoam;
numberOfProcessors 16;
tStart 170;
tEnd 250;
dt 5e-2;
writeTime 1;

// Flow parameters
Re_b 3333;
rho 1;
Ucl 1;
nu 0.0002;


// Turbulence model
model dynamicSmagorinsky;
cs 0.065;

// Orr-Sommerfeld Initial conditions 
alpha_2D 1.12;
alpha_3D 1.12;
beta_2D 0;
beta_3D 2.1;
A_2D 3;
A_3D 0.1;
n_2D 0;
n_3D 2;
Np 0;


// Meshing
H 2;
W 2.9919930042;
L 5.60998688141;
nx 32;
ny 32;
nz 32;
gradient 20;


