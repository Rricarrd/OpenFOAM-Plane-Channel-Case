FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs (
    "libOpenFOAM.so"
    "libsimpleSwakFunctionObjects.so"
    "libswakFunctionObjects.so"
    "libgroovyBC.so"
    "libdynamicSmagorinsky.so"
    "libsampling.so"
   );

application     pisoFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         200;

deltaT          0.03;

maxCo           1;

writeControl    timeStep;

writeInterval   33.333333333333336;

writeFormat     binary;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable true;


runTimeModifiable true;
adjustTimeStep  true;




functions
{
    
    // Calculating extra fields and some averages
    #include "fieldsCalculation"
    #include "fieldsAverageCalculation"

    // Continuous sampling functions
    // #include "samplePoints"
    #include "pressureDiff"
     

     



}


