// Simulation parameters
case temporal;
solver pisoFoam;
numberOfProcessors 16;
tStart 0;
tEnd 10;
dt 5e-3;

// Turbulence model
model dynamicSmagorinsky;
ce 1.048;
ck 0.0265463553;

// Orr-Sommerfeld Initial conditions 
Ucl 10;
alpha_2D 1.12;
alpha_3D 1.12;
beta_2D 0;
beta_3D 2.1;
A_2D 3;
A_3D 0.1;
n_2D 0;
n_3D 2;
Np 0;
N 32;
Reb 3333;
dynVisc 2.5494595145829e-3;
rho 1;

// Meshing
H 1;
H2 2;
W 3;
L 50;
nx 200;
ny 32;
nz 32;
resFactor 1.;
graded true;


